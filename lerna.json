{
  "command": {
    "run": {
      "stream": true,
      "sort": true
    },
    "publish": {
      "registry": "https://npm.pkg.github.com"
    },
    "version": {
      "allowBranch": [
        "main",
        "*-beta",
        "*-release",
        "beta-*",
        "release-*",
        "release/*",
        "hotfix/*"
      ],
      "conventionalCommits": true
    }
  },
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "version": "1.3.1-beta.0",
  "packages": [
    "packages/*"
  ],
  "npmClient": "npm"
}