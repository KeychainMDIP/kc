{
  "name": "@mdip/common",
  "version": "1.4.0-beta.5",
  "description": "MDIP common",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./utils": {
      "import": "./dist/esm/utils.js",
      "require": "./dist/cjs/utils.cjs",
      "types": "./dist/types/utils.d.ts"
    },
    "./errors": {
      "import": "./dist/esm/errors.js",
      "require": "./dist/cjs/errors.cjs",
      "types": "./dist/types/errors.d.ts"
    },
    "./logger": {
      "import": "./dist/esm/logger.js",
      "require": "./dist/cjs/logger.cjs",
      "types": "./dist/types/logger.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "utils": [
        "./dist/types/utils.d.ts"
      ],
      "errors": [
        "./dist/types/errors.d.ts"
      ],
      "logger": [
        "./dist/types/logger.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build:esm": "tsc -p tsconfig.json",
    "bundle:cjs": "rollup -c rollup.cjs.config.js",
    "build": "npm run clean && npm run build:esm && npm run bundle:cjs"
  },
  "keywords": [],
  "author": "David McFadzean <davidmc@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "pino": "^9.0.0",
    "pino-pretty": "^10.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KeychainMDIP/kc.git"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "rollup": "^4.40.2"
  }
}
