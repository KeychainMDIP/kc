{
  "name": "@mdip/gatekeeper",
  "version": "1.1.1",
  "description": "MDIP Gatekeeper",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/gatekeeper.js",
      "require": "./dist/cjs/gatekeeper.js",
      "types": "./dist/types/gatekeeper.d.ts"
    },
    "./client": {
      "import": "./dist/esm/gatekeeper-client.js",
      "require": "./dist/cjs/gatekeeper-client.js",
      "types": "./dist/types/gatekeeper-client.d.ts"
    },
    "./db/json": {
      "import": "./dist/esm/db/json.js",
      "require": "./dist/cjs/db/json.js",
      "types": "./dist/types/db/json.d.ts"
    },
    "./db/json-cache": {
      "import": "./dist/esm/db/json-cache.js",
      "require": "./dist/cjs/db/json-cache.js",
      "types": "./dist/types/db/json-cache.d.ts"
    },
    "./db/json-memory": {
      "import": "./dist/esm/db/json-memory.js",
      "require": "./dist/cjs/db/json-memory.js",
      "types": "./dist/types/db/json-memory.d.ts"
    },
    "./db/sqlite": {
      "import": "./dist/esm/db/sqlite.js",
      "require": "./dist/cjs/db/sqlite.js",
      "types": "./dist/types/db/sqlite.d.ts"
    },
    "./db/redis": {
      "import": "./dist/esm/db/redis.js",
      "require": "./dist/cjs/db/redis.js",
      "types": "./dist/types/db/redis.d.ts"
    },
    "./db/mongo": {
      "import": "./dist/esm/db/mongo.js",
      "require": "./dist/cjs/db/mongo.js",
      "types": "./dist/types/db/mongo.d.ts"
    },
    "./types": {
      "import": "./dist/types/types.d.js",
      "require": "./dist/types/types.d.js",
      "types": "./dist/types/types.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build:esm": "tsc -p tsconfig.json",
    "bundle:cjs": "rollup -c rollup.cjs.config.js",
    "build": "npm run clean && npm run build:esm && npm run bundle:cjs"
  },
  "keywords": [],
  "author": "David McFadzean <davidmc@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@mdip/cipher": "*",
    "@mdip/common": "*",
    "@mdip/ipfs": "*",
    "axios": "^1.7.7",
    "canonicalize": "^2.0.0",
    "dotenv": "^16.4.5",
    "ioredis": "^5.4.1",
    "mongodb": "^6.5.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KeychainMDIP/kc.git"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0"
  }
}
