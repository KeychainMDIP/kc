{
  "name": "@mdip/inscription",
  "version": "1.2.1",
  "description": "MDIP Inscription",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./types": {
      "types": "./dist/types/types.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "inscription": [
        "./dist/types/inscription.d.ts"
      ],
      "types": [
        "./dist/types/types.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build:esm": "tsc -p tsconfig.json",
    "bundle:cjs": "rollup -c rollup.cjs.config.js",
    "build": "npm run clean && npm run build:esm && npm run bundle:cjs"
  },
  "keywords": [],
  "author": "Peter Bushnell <bushsolo@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KeychainMDIP/kc.git"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "rollup": "^4.40.2"
  },
  "dependencies": {
    "bip32": "^5.0.0-rc.0",
    "bitcoinjs-lib": "^6.1.7",
    "ecpair": "^3.0.0",
    "tiny-secp256k1": "^2.2.4",
    "varuint-bitcoin": "^2.0.0"
  }
}
