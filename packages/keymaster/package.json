    "ioredis": "^5.4.1",
    "mongodb": "^6.5.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KeychainMDIP/kc.git"
