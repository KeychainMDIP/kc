{
  "name": "@mdip/ipfs",
  "version": "1.4.0-beta.5",
  "description": "MDIP IPFS lib",
  "type": "module",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.cjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "browser": {
        "import": "./dist/esm/index.js",
        "types": "./dist/types/index.d.ts"
      },
      "node": {
        "import": "./dist/esm/node.js",
        "require": "./dist/cjs/node.cjs",
        "types": "./dist/types/node.d.ts"
      },
      "default": {
        "import": "./dist/esm/node.js",
        "types": "./dist/types/node.d.ts"
      }
    },
    "./helia": {
      "import": "./dist/esm/helia-client.js",
      "require": "./dist/cjs/helia-client.cjs",
      "types": "./dist/types/helia-client.d.ts"
    },
    "./kubo": {
      "import": "./dist/esm/kubo-client.js",
      "require": "./dist/cjs/kubo-client.cjs",
      "types": "./dist/types/kubo-client.d.ts"
    },
    "./cluster": {
      "import": "./dist/esm/cluster-client.js",
      "require": "./dist/cjs/cluster-client.cjs",
      "types": "./dist/types/cluster-client.d.ts"
    },
    "./utils": {
      "import": "./dist/esm/utils.js",
      "require": "./dist/cjs/utils.cjs",
      "types": "./dist/types/utils.d.ts"
    },
    "./types": {
      "types": "./dist/types/types.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "helia": [
        "./dist/types/helia-client.d.ts"
      ],
      "kubo": [
        "./dist/types/kubo-client.d.ts"
      ],
      "cluster": [
        "./dist/types/cluster-client.d.ts"
      ],
      "utils": [
        "./dist/types/utils.d.ts"
      ],
      "types": [
        "./dist/types/types.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build:esm": "tsc -p tsconfig.json",
    "bundle:cjs": "rollup -c rollup.cjs.config.js",
    "build": "npm run clean && npm run build:esm && npm run bundle:cjs"
  },
  "keywords": [],
  "author": "David McFadzean <davidmc@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@helia/json": "^4.0.3",
    "@helia/unixfs": "^5.0.0",
    "axios": "^1.7.7",
    "blockstore-fs": "^2.0.2",
    "helia": "^5.3.0",
    "ip": "^2.0.1",
    "kubo-rpc-client": "^5.1.0",
    "@mdip/common": "*",
    "multiformats": "^13.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KeychainMDIP/kc.git"
  },
  "devDependencies": {
    "@types/ip": "^1.1.3"
  }
}
