{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "plugins": ["sonarjs", "only-warn"],
    "extends": ["plugin:sonarjs/recommended-legacy","react-app"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "warn",
            4
        ]

    }
}